{
  "name": "@marionebl/babel-live",
  "version": "1.3.0",
  "description": "Live reloading of servers compiled via babel",
  "scripts": {
    "start": "npm run build -- --watch",
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      "es2015-node4",
      "stage-0"
    ]
  },
  "files": [
    "index.js",
    "lib"
  ],
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-types": "^6.10.2",
    "babylon": "^6.8.2",
    "chokidar": "^1.4.1",
    "resolve": "^1.1.7"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015-node4": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marionebl/babel-live.git"
  },
  "license": "MIT"
}
